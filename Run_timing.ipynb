   "source": [
    "time_size = 0\n",
    "total_time = 0\n",
    "\n",
    "while True:\n",
    "\n",
    "    time = input('Enter the minutes it took for 10km run today : ').upper()\n",
    "    if time == 'Q':\n",
    "        break\n",
    "    else:\n",
    "        total_time += float(time)\n",
    "        time_size += 1\n",
    "avg_time = total_time/time_size\n",
    "print(avg_time)"
   ]
